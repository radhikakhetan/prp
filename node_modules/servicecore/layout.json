{
    "root": "services",
    "layout": {
        "protocol": "https:",
        "basepath": "/",
        "keepAlive": false,
        "socketTimeout": 10000,
        "connectTimeout": 200,
        "maxRetry": 3,
        "ciphers": "AES128-SHA256:AES128-SHA:3DES+SHA1",
        "secureProtocol": "SSLv23_method"
    }
}
